import java.util.*;
public class stack{

    public static void main(String args[]){
       Stack<String> al=new Stack<String>();
        al.add("Apple");
        al.add("Mango");
        al.add("Orange");
        al.add("Banana");
        al.remove("Mango");
        Iterator<String> itr=al.iterator();
        while(itr.hasNext()){
            System.out.println(itr.next());
        }
    }
}
