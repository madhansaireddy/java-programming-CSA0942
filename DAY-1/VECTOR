import java.util.*;
public class vector{

    public static void main(String args[]){
        Vector<String> al=new Vector<String>();
        al.add("Apple");
        al.add("Mango");
        al.add("Orange");
        al.add("Banana");
        al.remove("Orange");
        Iterator<String> itr=al.iterator();
        while(itr.hasNext()){
            System.out.println(itr.next());
        }
    }
}
